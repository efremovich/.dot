{
 "suggest.maxPreviewWidth": 120,
 "suggest.noselect": false,

 "coc.preferences.formatOnSaveFiletypes": [
    "javascript",
    "typescript",
    "typescriptreact",
    "json",
    "vue",
    "php"
  ],
  "languageserver": {
  "golang": {
      "command": "gopls",
      "rootPatterns": ["go.mod"],
      "disableWorkspaceFolders": true,
      "filetypes": ["go"]
    }
  }

}


